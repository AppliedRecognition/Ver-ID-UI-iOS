target 'VerIDUI' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for VerIDUI
  pod 'Ver-ID-Core', '~> 1.6'

end

target 'Ver-ID Sample' do
  use_frameworks!
  pod 'Ver-ID-Core', '~> 1.6'
end
